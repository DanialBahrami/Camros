{
	"name": "pip_lake_test",
	"properties": {
		"activities": [
			{
				"name": "restToblob",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "RestSource",
						"httpRequestTimeout": "00:01:40",
						"requestInterval": "00.00:00:00.010",
						"requestMethod": "GET"
					},
					"sink": {
						"type": "JsonSink",
						"storeSettings": {
							"type": "AzureBlobStorageWriteSettings",
							"copyBehavior": "FlattenHierarchy"
						},
						"formatSettings": {
							"type": "JsonWriteSettings",
							"filePattern": "setOfObjects"
						}
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "ds_bo_sap_invoices",
						"type": "DatasetReference",
						"parameters": {
							"resPath": {
								"value": "@concat(variables('resource'), '?', variables('queryparam'))",
								"type": "Expression"
							}
						}
					}
				],
				"outputs": [
					{
						"referenceName": "ds_blob_bondura",
						"type": "DatasetReference",
						"parameters": {
							"Directory": {
								"value": "@variables('resource')",
								"type": "Expression"
							},
							"FileName": {
								"value": "@concat('records_',variables('queryparam'),'.json')",
								"type": "Expression"
							}
						}
					}
				]
			}
		],
		"variables": {
			"resource": {
				"type": "String",
				"defaultValue": "Invoices"
			},
			"queryparam": {
				"type": "String",
				"defaultValue": "$skip=2700"
			}
		},
		"annotations": []
	}
}