{
	"name": "pipeline_copy_SAP",
	"properties": {
		"description": "Getting Data from SAP OData Layer, put in SQL DB, without any transformation",
		"activities": [
			{
				"name": "ForEach_oap",
				"type": "ForEach",
				"dependsOn": [],
				"userProperties": [],
				"typeProperties": {
					"items": {
						"value": "@pipeline().parameters.cw_items",
						"type": "Expression"
					},
					"activities": [
						{
							"name": "Copy_oap",
							"type": "Copy",
							"dependsOn": [],
							"policy": {
								"timeout": "0.12:00:00",
								"retry": 0,
								"retryIntervalInSeconds": 30,
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [
								{
									"name": "Source",
									"value": "@{item().source.path}"
								},
								{
									"name": "Destination",
									"value": "dbo.@{item().destination.table}"
								}
							],
							"typeProperties": {
								"source": {
									"type": "ODataSource"
								},
								"sink": {
									"type": "AzureSqlSink",
									"tableOption": "autoCreate"
								},
								"enableStaging": false,
								"enableSkipIncompatibleRow": true
							},
							"inputs": [
								{
									"referenceName": "SourceDataset_oap",
									"type": "DatasetReference",
									"parameters": {
										"cw_path": "@item().source.path"
									}
								}
							]
						}
					]
				}
			}
		],
		"parameters": {
			"cw_items": {
				"type": "Array",
				"defaultValue": [
					{
						"source": {
							"path": "BudgetDistributions"
						},
						"destination": {
							"table": "BudgetDistributions"
						}
					},
					{
						"source": {
							"path": "Budgets"
						},
						"destination": {
							"table": "Budgets"
						}
					},
					{
						"source": {
							"path": "BudgetScenarios"
						},
						"destination": {
							"table": "BudgetScenarios"
						}
					},
					{
						"source": {
							"path": "ChartOfAccounts"
						},
						"destination": {
							"table": "ChartOfAccounts"
						}
					},
					{
						"source": {
							"path": "Invoices"
						},
						"destination": {
							"table": "Invoices"
						}
					},
					{
						"source": {
							"path": "Orders"
						},
						"destination": {
							"table": "Orders"
						}
					}
				]
			}
		},
		"annotations": [],
		"lastPublishTime": "2024-04-15T15:24:08Z"
	},
	"type": "Microsoft.DataFactory/factories/pipelines"
}